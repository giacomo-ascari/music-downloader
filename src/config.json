{
    "base_url": "/music-downloader",
    "public_folder": "public",
    "ignorable_headers": [
        "user-agent",
        "accept",
        "postman-token",
        "host",
        "accept-encoding",
        "connection",
        "content-length",
        "cache-control",
        "sec-ch-ua",
        "sec-ch-ua-mobile",
        "upgrade-insecure-requests",
        "sec-fetch-site",
        "sec-fetch-mode",
        "sec-fetch-user",
        "sec-fetch-dest",
        "accept-language",
        "cookie"
    ],
    "platforms": {
        "youtube": "https://www.youtube.com/watch?v=$"
    },
    "req_attr": {
        "track_code": "t_code",
        "track_platform": "t_plat"
    },
    "tracks": {
        "folder":"./tracks",
        "format":"webm",
        "temp_f":"temp"
    }
}